{
  "name": "football-journalism",
  "version": "2.2.0",
  "private": true,
  "engines": {
    "npm": ">=8.19.4",
    "node": ">=16.20.0"
  },
  "description": "The app is a platform for self-identified journalists around the world with information about soccer to exchange information.  We tried a loosely coupled architecture using AWS serverless.  Enhancements also integrated AI services to break down language barriers and visual and hearing impairments.",
  "author": "ShotaroMatsuya",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShotaroMatsuya/football-journalism.git"
  },
  "bugs": {
    "url": "https://github.com/ShotaroMatsuya/football-journalism/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lint-fix": "eslint --cache --fix src/ && prettier --write src/",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,json,vue}'",
    "prettier:write": "prettier --write 'src/**/*.{js,jsx,json,vue}'",
    "test:coverage": "vue-cli-service test:unit --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges --output './badges'"
  },
  "dependencies": {
    "core-js": "^3.29.1",
    "vue": "^3.2.13",
    "vue-router": "^4.1.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "cypress": "^13.0.0",
