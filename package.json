    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "firebase-tools": "^13.0.0",
    "flush-promises": "^1.0.2",
    "jest": "^27.0.5",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^3.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "^3.8.3"
